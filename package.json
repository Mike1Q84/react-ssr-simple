{
  "name": "react-ssr-simple",
  "version": "0.0.1",
  "description": "Simple server-side rendering React without Redux",
  "main": "server.js",
  "scripts": {
    "clean": "rimraf dist",
    "serve": "nodemon server.js",
    "dev": "run-p clean dev:bundle serve",
    "dev:bundle": "webpack -w",
    "prod": "run-p clean prod:bundle",
    "prod:bundle": "NODE_ENV=production webpack -p",
    "jest": "jest --coverage --watch",
    "test": "karma start"
  },
  "jest": {
    "setupFiles": [
      "raf/polyfill"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|scss|sass)$": "identity-obj-proxy"
    },
    "testRegex": "\\.test\\.jsx?$"
  },
  "keywords": [
    "React",
    "SSR",
    "TDD+BDD",
    "Karma",
    "Mocha",
    "Quixote"
  ],
  "author": "Mike Zhang",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^7.1.4",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "chai": "^4.1.2",
    "chai-enzyme": "^0.8.0",
    "css-loader": "^0.28.7",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.1",
    "eslint": "^4.8.0",
    "eslint-plugin-react": "^7.4.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^1.1.4",
    "identity-obj-proxy": "^3.0.0",
    "ignore-styles": "^5.0.1",
    "jest": "^21.2.1",
    "karma": "^1.7.1",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.4",
    "mocha": "^4.0.0",
    "node-sass": "^4.5.3",
    "nodemon": "^1.12.1",
    "npm-run-all": "^4.1.1",
    "null-loader": "^0.1.1",
    "postcss-loader": "^2.0.6",
    "quixote": "^0.12.5",
    "raf": "^3.4.0",
    "react-addons-test-utils": "^15.6.2",
    "react-test-renderer": "^16.0.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "webpack": "^3.6.0",
    "webpack-node-externals": "^1.6.0"
  },
  "dependencies": {
    "express": "^4.16.1",
    "normalize.css": "^7.0.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-helmet": "^5.2.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "redis": "^2.8.0",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "serialize-javascript": "^1.4.0"
  }
}

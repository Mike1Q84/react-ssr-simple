{
  "name": "react-ssr-simple",
  "version": "0.0.1",
  "description": "Simple server-side rendering React without Redux",
  "main": "server.js",
  "scripts": {
    "clean": "rimraf dist",
    "serve": "nodemon server.js",
    "dev": "run-p clean dev:bundle serve",
    "dev:bundle": "webpack -w",
    "prod": "run-p clean prod:bundle",
    "prod:bundle": "NODE_ENV=production webpack -p",
    "test": "karma start"

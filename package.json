    "babel-preset-react": "^6.24.1",
    "chai": "^4.1.2",
    "chai-enzyme": "^0.8.0",
    "css-loader": "^0.28.7",
    "enzyme": "^2.9.1",
    "eslint": "^4.6.0",
    "eslint-plugin-react": "^7.3.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "ignore-styles": "^5.0.1",
    "karma": "^1.7.1",
